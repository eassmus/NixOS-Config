{
  "position": "top",
  "mod": "dock",
  "exclusive": true,
  "passthrough": false,
  "gtk-layer-shell": true,
  "height": 0,
  "modules-left": [
    "battery",
    "network",
    "custom/wsleft",
    "custom/wsmiddle",
    "custom/wsright"
  ],
  "modules-center": [
    "clock"
  ],
  "modules-right": [
    "custom/cpu",
    "custom/gpu",
    "custom/temp",
    "custom/fan_mode",
    "pulseaudio",
    "pulseaudio#microphone"
  ],
  "custom/fan_mode": {
    "exec" : "asusctl profile --profile-get | tail -n +2 | cut -c 19-19 | /home/pulsar/.config/waybar/scripts/fan-speed.sh",
    "tooltip": false,
    "format" : "{} ",
    "interval": 1,
    "on-click": "asusctl profile --next",
    "on-click-right": "asusctl profile --next && asusctl profile --next"
  },
  "upower": {
    "format": "{percentage}%"
  },
  "custom/wsleft": {
    "tooltip": false,
    "format": "ÔÅ†",
    "on-click" : "hyprctl dispatch workspace -1",
    "on-click-right" : "hyprctl dispatch workspace -2"
  },
  "custom/wsmiddle" : {
    "tooltip": false,
    "exec": "hyprctl activeworkspace -j | grep '^    \"name\": \"' | cut -c14",
    "return-type": "",
    "format": "{}",
    "interval": 1
  },
  "custom/wsright" : {
    "tooltip": false,
    "format": "ÔÅ°",
    "on-click" : "hyprctl dispatch workspace +1",
    "on-click-right" : "hyprctl dispatch workspace +2"
  },
  "custom/cpu": {
    "exec": "~/.config/waybar/scripts/cpu_usage.sh",
    "on-click": "kitty -e btop",
    "format": "Ó∞ô {}%",
    "return-type": "",
    "interval": 1,
    "tooltip": false
  },
  "memory": {
    "format": "ÓøÖ {:>2}%",
    "on-click": "kitty -e btop",
    "interval": 1
  },
  "custom/gpu": {
    "exec": "nvidia-smi --query-gpu=utilization.gpu --format=csv,noheader,nounits",
    "on-click": "kitty -e nvtop",
    "format": "Û∞¢Æ {:>2}%",
    "return-type": "",
    "interval": 1,
    "tooltip": false
  },
  "custom/temp": {
    "exec": "head -c 2 /sys/class/thermal/thermal_zone0/temp",
    "on-click": "kitty -e btop",
    "format": "Óº™ {}",
    "return-type": "",
    "interval": 1,
    "tooltip": false
  },
  "custom/integratedgpu": {
    "exec": "~/.config/waybar/scripts/get_integrated_gpu_busy.sh",
    "on-click": "kitty -e nvtop",
    "format": "Û∞òö {:>2}%",
    "return-type": "",
    "interval": 1,
    "tooltip": false
  },
  "custom/brightness": {
    "exec": "~/.config/waybar/scripts/get_brightness.sh",
    "format": "Óåç {}%",
    "on-scroll-up": "~/.config/hypr/scripts/brightness_change.sh 3",
    "on-scroll-down": "~/.config/hypr/scripts/brightness_change.sh -3",
    "on-click-middle": "light -S 0",
    "return-type": "",
    "interval": 1,
    "min-length": 5,
    "tooltip": false
  },
  "network": {
    "interface": "wlp2s0",
    "format-wifi": "ÔÄí  | {bandwidthUpBits:>5} Û∞ïí | {bandwidthDownBits:>5} Û∞áö",
    "format-ethernet": "Û∞àÅ | {bandwidthUpBits:>5} Û∞ïí | {bandwidthDownBits:>5} Û∞áö",
    "format-disconnected": "Û±òñ ", 
    "format": "Û∞§â | {bandwidthUpBits:>5} Û∞ïí | {bandwidthDownBits:>5} Û∞áö",
    "format-disabled": "",
    "interval": 1,
    "tooltip": true,
    "tooltip-format": " {essid:.20} | {signalStrength}% ",
    "on-click": "kitty -e nmtui"
  },
  "hyprland/workspaces": {
    "persistent_workspaces": {
      "1": []
    }
  },
  "custom/kbbacklight": {
    "exec": "brightnessctl -d asus::kbd_backlight g",
    "format": "ÔÑú  {}/3",
    "return-type": "",
    "interval": 1,
    "tooltip": false
  },
  "tray": {
    "icon-size": 16,
    "spacing": 10
  },
  "clock": {
    "format": "{:%I:%M %p | %A | %m-%d-%Y}",
    "min-length": 32,
    "tooltip": true,
    "interval": 60,
    "tooltip-format": "{calendar}",
    "on-click-middle": "~/.config/hypr/scripts/wofi_toggle.sh",
	  "calendar": {
		  "mode"          : "month",
		  "mode-mon-col"  : 3,
		  "weeks-pos"     : "",
		  "on-scroll"     : 1,
		  "format": {
			  "months":     "<span color='#82aee8' ><b>{}</b></span>",
			  "days":       "<span><b>{}</b></span>",
        "weeks": "<span><b></b></span>",
			  "weekdays":   "<span color='#e9a6fd'><b >{}</b></span>",
			  "today":      "<span color='#e9a6fd'><b>{}</b></span>"
		  }
	  },
    "actions": {
      "on-click-right": "shift_down",
      "on-click": "shift_up"
    }
  },
  "pulseaudio": {
    "format": "ÔÄ¶ {volume}%",
    "format-muted": "ÔÄ¶ Muted",
    "on-click": "pamixer -t",
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "ÔÄ•",
      "headset": "ÔÄ•",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ"
    },
    "min-length": 7
  },
  "pulseaudio#microphone": {
    "format": "{format_source}",
    "format-source": "üéô {volume}%",
    "format-source-muted": "üéô Muted",
    "on-click": "pamixer --default-source -t",
    "min-length": 8
  },
  "battery": {
    "bat": "BAT0",
    "interval": 1,
    "format": "Ó∫û {capacity}% | {time}",
    "format-plugged": "Û∞ö• {capacity}%",
    "format-not-charging": "Û∞ö• {capacity}%",
    "format-charging": "ÔÉß {capacity}% | {time}",
    "format-time": "{H}:{m}",
    "states": {
      "warning": 30,
      "critical": 10
    },
    "tooltip": true,
    "tooltip-format": " Power: {power:.0f}W ",
    "on-click": "wlogout"
  },
  "battery#bat2": {
    "bat": "hidpp_battery_0",
    "interval": 1,
    "format": "Û∞çΩ {capacity}%",
    "states": {
      "warning": 30,
      "critical": 10
    }
  }
}
